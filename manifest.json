{
	"manifest_version": 2,
  	"name": "Aureal App",
    "description": "Podcast rating platform",
    "version": "0.0.0.1",
    "permissions" : [
    		"bookmarks",
        "storage",
        "sessions",
        "tabs",
        "chrome://favicon/"],
    "chrome_url_overrides" : {
        "newtab": "newpage.html"
    },
    	"background": {
		"scripts":["background.js"],
		"persistent": false
	}
 }